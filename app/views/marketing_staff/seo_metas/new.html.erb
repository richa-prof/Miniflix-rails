<div class="container" id="amenity-new">
  <h2></h2>
  <%= form_for @seo_meta, url: marketing_staff_seo_metas_path do |form| %>
    <% if @seo_meta.errors.any? %>
      <div id="error_explanation">
        <h2>
          <%= pluralize(@seo_meta.errors.count, "error") %>
          prohibited this seo_meta from being saved:
        </h2>
        <ul>
          <% @seo_meta.errors.full_messages.each do |message| %>
            <li>
              <%= message %>
            </li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <%= render 'marketing_staff/seo_metas/form', form: form %>
  <% end %>
</div>
