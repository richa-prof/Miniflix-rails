<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    Listing Seo Metas
  </h1>
</section>

<!-- Main content -->
<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box">
        <div class="box-header">
          <%= render( partial: 'shared/flash_notification', locals: { notice: notice } ) %>
          <div class="row">
            <div class="col-lg-10 col-xs-12">
            </div>
            <div class="col-lg-2 col-xs-12">
              <%= link_to 'New Seo Meta', new_marketing_staff_seo_meta_path, class: "btn btn-primary" %>
            </div>
          </div>
        </div>
        <!-- /.box-header -->
        <div class="box-body admin-box-body">
          <div class="row">
            <div class="col-md-12">
              <!-- general form elements -->
              <div class="box box-primary">
                <!-- /.box-header -->
                <div class="box-body">
                  <table id="tbl_seo_metas" class="table table-bordered table-hover">
                    <thead id="location_header">
                      <%= render 'marketing_staff/seo_metas/table_header' %>
                    </thead>
                    <tbody id="location_list">
                      <%= render partial: 'marketing_staff/seo_metas/seo_meta', collection: @seo_metas, as: :seo_meta %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->
    </div>
    <!-- /.col -->
  </div>
  <!-- /.row -->
</section>
<!-- /.content -->

<script>
  $(document).ready(function() {
    $('#tbl_seo_metas').dataTable({
      "paging": true,
      "lengthChange": true,
      "searching": true,
      "ordering": true,
      "info": true
    });
  });
</script>
